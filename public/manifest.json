{
  "manifest_version": 3,
  "name": "TabSmart",
  "version": "1.0.0",
  "description": "Efficiently manage multiple tabs",
  "permissions": ["tabs", "tabGroups"],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
